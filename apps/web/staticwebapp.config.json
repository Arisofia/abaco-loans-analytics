{
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/bd9ff25a-6624-43cb-aea8-34f3dc751ef3/",
          "clientIdSettingName": "AZURE_AD_CLIENT_ID",
          "clientSecretSettingName": "AZURE_AD_CLIENT_SECRET"
        }
      }
    }
  },
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad",
      "statusCode": 302
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/static/*", "/_next/*"]
  }
}
